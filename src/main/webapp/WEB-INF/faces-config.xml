<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                                  http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<navigation-rule>
		<from-view-id>/index.xhtml</from-view-id>
		
		<navigation-case>
			<from-outcome>ircadastro</from-outcome>
			<to-view-id>/Pcadastro.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>irhome</from-outcome>
			<to-view-id>/Phome.xhtml</to-view-id>
		</navigation-case>
		
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/Pcadastro.xhtml</from-view-id>
		
		<navigation-case>
			<from-outcome>irindex</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/Phome.xhtml</from-view-id>
		
		<navigation-case>
			<from-outcome>irindex</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>irperfil</from-outcome>
			<to-view-id>/Pperfil.xhtml</to-view-id>
		</navigation-case>
		
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/Pperfil.xhtml</from-view-id>
		
		<navigation-case>
			<from-outcome>irindex</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>irhome</from-outcome>
			<to-view-id>/Phome.xhtml</to-view-id>
		</navigation-case>
		
	</navigation-rule>


</faces-config>
